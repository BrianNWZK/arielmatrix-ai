{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "api/cosmoweb3db.py", "use": "@vercel/python", "config": { "runtime": "python3.12" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/cosmoweb3db.py" },
    { "src": "/deals", "dest": "/deals.html" },
    { "src": "/deals.html", "dest": "/deals.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": false
}
