{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/cosmoweb3db.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.12" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/cosmoweb3db.py"
    },
    {
      "src": "/deals.html",
      "dest": "/dist/deals.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "env": {
    "VITE_BSC_PRIVATE_KEY": ""
  },
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": false
}
