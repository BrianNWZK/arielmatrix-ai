{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "api/cosmoweb3db.py", "use": "@vercel/python", "config": { "runtime": "python3.12" } },
    { "src": "api/trafficbot.py", "use": "@vercel/python", "config": { "runtime": "python3.12" } },
    { "src": "api/orchestrator.py", "use": "@vercel/python", "config": { "runtime": "python3.12" } }
  ],
  "routes": [
    { "src": "/api/cosmoweb3db/?(.*)", "dest": "/api/cosmoweb3db.py" },
    { "src": "/api/trafficbot/?(.*)", "dest": "/api/trafficbot.py" },
    { "src": "/api/orchestrator/?(.*)", "dest": "/api/orchestrator.py" },
    { "src": "/deals.html", "dest": "/dist/deals.html" },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html",
      "headers": {
        "Cache-Control": "no-store, no-cache, must-revalidate, proxy-revalidate",
        "Pragma": "no-cache",
        "Expires": "0"
      }
    }
  ],
  "env": {
    "VITE_BSC_PRIVATE_KEY": "",
    "VITE_TRUST_WALLET_API_KEY": "",
    "VITE_BSCSCAN_API_KEY": "",
    "VITE_GROQ_API_KEY": ""
  },
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": false
}
