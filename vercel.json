{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "api/cosmoweb3db.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.12" }
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/cosmoweb3db.py"
    },
    {
      "src": "/deals",
      "dest": "/dist/deals.html"
    },
    {
      "src": "/deals.html",
      "dest": "/dist/deals.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "env": {
    "VITE_BSC_PRIVATE_KEY": "",
    "VITE_TRUST_WALLET_API_KEY": ""
  },
  "cleanUrls": true,
  "trailingSlash": false
}
