{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "api/cosmoweb3db.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.12" }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/cosmoweb3db.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "VITE_BSC_PRIVATE_KEY": ""
  },
  "outputDirectory": "dist"
}
